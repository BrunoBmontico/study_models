<odoo>

    <!-- Module Config > CEPs Tree -->
    <record id="tree_queue_ceps" model="ir.ui.view">
        <field name="name">tree.queue.ceps</field>
        <field name="model">queue.cep</field>
        <field name="arch" type="xml">
        
            <tree>
                <field name="name"/>
            </tree>
        
        </field>
    </record>

    <!-- Module Config > Districts Tree -->
    <record id="tree_queue_districts" model="ir.ui.view">
        <field name="name">tree.queue.districts</field>
        <field name="model">queue.district</field>
        <field name="arch" type="xml">
        
            <tree>
                <field name="name"/>
                <field name="cep_ids"/>
            </tree>
        
        </field>
    </record>

</odoo>